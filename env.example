# Database Configuration
CONNECTION_STRING=Server=localhost;Database=DeliveryApp;Trusted_Connection=true;TrustServerCertificate=true;MultipleActiveResultSets=true

# JWT Configuration
JWT_SECRET_KEY=your-super-secure-jwt-secret-key-here
JWT_ISSUER=DeliveryApp
JWT_AUDIENCE=DeliveryAppMobile

# API Configuration
API_SELF_URL=https://localhost:44357
API_CORS_ORIGINS=https://*.DeliveryApp.com,http://localhost:4200,http://localhost:44307,https://localhost:44307,https://localhost:44356

# Frontend URLs
FRONTEND_URL=http://localhost:4200
WEB_URL=https://localhost:44355
BLAZOR_URL=https://localhost:44370
SWAGGER_URL=https://localhost:44343

# Redis Configuration
REDIS_CONFIGURATION=127.0.0.1

# OAuth Configuration
OAUTH_CLIENT_SECRET=your-oauth-client-secret
OAUTH_SWAGGER_CLIENT_SECRET=your-swagger-client-secret

# String Encryption
STRING_ENCRYPTION_PASSPHRASE=your-encryption-passphrase

# SendPulse Configuration
SENDPULSE_CLIENT_ID=your_sendpulse_client_id
SENDPULSE_CLIENT_SECRET=your_sendpulse_client_secret
SENDPULSE_FROM_EMAIL=noreply@yourdeliveryapp.com
SENDPULSE_FROM_NAME=Delivery App

# Firebase Configuration
FIREBASE_SERVICE_ACCOUNT_PATH=
